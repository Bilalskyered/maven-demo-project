pipeline{
    agent any
    
    stages{
        stage("build java code"){
            steps{
                sh """
                rm -rf ./*
                git clone https://github.com/Bilalskyered/maven-demo-project.git
                cd maven-demo-project
                mvn package
                cd target
                7z a -t7z maven-demo-project.7z *.jar
                """
                echo "Jar file Deployed"
            }

        }
    }

}