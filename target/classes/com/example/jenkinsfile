pipeline{
    agent any
    
    stages{
        stage("build java code"){
            steps{
                sh """
                rm -rf ./*
                git clone https://github.com/Bilalskyered/maven-demo-project.git
                cd maven-demo-project
                mvn -X package
                cd target
                7zip -a maven-demo-project.zip *.jar
                """
            }

        }
    }

}